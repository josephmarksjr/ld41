pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- ludum dare 41
-- combine two
-- incompatibale genres

tools = {
  {
  	 index = 1,
    name = "hoe",
    sprite = 17,
    x = 8,
    y = 8,
    c = 6
  },
  {
  		index = 2,
    name = "watering can",
    sprite = 19,
    x = 16,
    y = 8,
    c = 12
  },
  {
  		index = 3,
    name = "seeds",
    sprite = 20,
    x = 24,
    y = 8,
    c = 11
  },
  {
  		index = 4,
    name = "sycthe",
    sprite = 21,
    x = 32,
    y = 8,
    c = 11
  }
}


p = {
		x = 64,
		y = 100,
		h = 4,
		w = 20
}

b = {
		x = 64,
		y = 64,
	 r = 3,
	 vx = -2,
	 vy = -2,
	 tool = tools[1]
}


rows = {
}

function _init()
		b.tool = tools[1]
		for i=1,16 do
  		row = {}
  		for j=1,16 do
  				add(row, {index=1})
  		end
  		add(rows,row)
		end	
end

function _update()	
		if (btn(0)) then
				p.x -= 2
		end
		
		if (btn(1)) then
				p.x += 2
		end
		
		if (btnp(2)) then
		  if (b.tool.index == 4) then
		    b.tool = tools[1]
		  else
		    b.tool = tools[b.tool.index + 1]
		  end
		end
		
		if (
				b.y >= p.y and
				b.y <= p.y + p.h and
				b.x >= p.x and
				b.x <= p.x + p.w
		) then
				b.vy -= 2
		end
		
		if (b.y <= 0) then
		  b.vy += 2
		end
		
		if (b.x <= 0) then
				b.vx += 2
		end
		
		if (b.x >= 128) then
				b.vx -= 2
		end
		
		b.x += b.vx
		b.y += b.vy
		
		-- access cell you are in
		local x = flr(b.x/8) + 1
		local y = flr(b.y/8) + 1
		if (
				x > 0 and 
				x < 16 and 
				y > 0 and 
				y < 16
		) then
		  if (rows[x][y] and b.tool) then
		    new_cell = rows[x][y]
		    if (b.tool.name == "hoe") then
		    		new_cell = do_hoe(rows[x][y])
		    end
		    
		    if (b.tool.name == "seeds") then
		      new_cell = do_seeds(rows[x][y])
		    end  
		    
		    rows[x][y] = new_cell
		  end
		end
end

function do_hoe(cell)
  if (cell.index != 1) then
    return cell
  end
  
  cell.index = 2
  
  return cell
end

function do_seeds(cell)
  if (cell.index != 2) then
    return cell
  end
  
  cell.index = 3
  
  return cell
end

function _draw()
		cls()
		
  for i=1,16 do
				for j=1,16 do
					 local cell = rows[i][j] 
					 -- do stuff with the cell
					 spr(cell.index,(i*8)-8,(j*8)-8)
				end
		end
		
		circfill(8, 120, 12, 15)
		sspr(b.tool.x, b.tool.y, 8, 8, 1, 110, 16, 16)
		
		circfill(b.x, b.y, b.r, b.tool.c)
		rectfill(p.x, p.y, p.x+p.w, p.y+p.h, 10)
  pset(b.x,b.y)
  print(p.x)
  print(p.y)
end
__gfx__
