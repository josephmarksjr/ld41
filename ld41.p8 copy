pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- ludum dare 41
-- combine two
-- incompatibale genres

level_goals = {
		1000,
		5000,
		10000,
		50000,
		100000
}

level = 0
cash = 0
goal = 1000

function init_level(level)
	 rows = {}
  level = level
  cash = 0
  goal = level_goals[level+1]
  
  b.tool = tools[1]
		for i=1,16 do
  		row = {}
  		for j=1,16 do
  			 if (j < 12) then
  				  rand = mget((level*16)+i-1,j-1)
  				  if (rand == 0) then
  				    rand = 1
  				  end
  				else
  				  rand = 1
  				end
  				add(row, {
  						index=rand,--beginning_sprites[rand + 1],
  						water_count = 0,
  						ready_count = 100
  				})
  		end
  		add(rows,row)
		end	
end

tools = {
  {
  	 index = 1,
    name = "hoe",
    sprite = 64,
    c = 6
  },
  {
  		index = 2,
    name = "watering can",
    sprite = 66,
    c = 12
  },
  {
  		index = 3,
    name = "seeds",
    sprite = 68,
    c = 11
  },
  {
  		index = 4,
    name = "pickaxe",
    sprite = 70,
    c = 11
  },
  {
  		index = 5,
    name = "scythe",
    sprite = 72,
    c = 11
  }
}


p = {
		x = 64,
		y = 110,
		h = 4,
		w = 20,
		c = 2
}

b = {
		x = 64,
		y = 128,
	 r = 3,
	 dx = 2,
	 dy = 2,
	 dv = 0.1,
	 tool = tools[1]
}


rows = {
}

function _init()
	 init_level(0)	
end

function _update()
  if (btn(4) and cash>goal)then
    init_level(level+1)
  end
  
  if (btn(5)) then
    cash += 100
  end

		if (btn(0)) then
				p.x -= 4
		end
		
		if (btn(1)) then
				p.x += 4
		end
		
		if (btnp(2)) then
		  if (b.tool.index == 5) then
		    b.tool = tools[1]
		  else
		    b.tool = tools[b.tool.index + 1]
		  end
		end
		
		if (btnp(3)) then
		  if (b.tool.index == 1) then
		    b.tool = tools[5]
		  else
		    b.tool = tools[b.tool.index - 1]
		  end
		end
		
		next_x = b.x + b.dx
		next_y = b.y + b.dy
		
		if (next_x >= 128 or next_x < 0) then
				next_x = mid(0,next_x,127)
				b.dx = -b.dx
				sfx(0)
		end
		
		if (next_y >= 128 or next_y < 0) then
				next_y = mid(0,next_y,127)
				b.dy = -b.dy
				sfx(0)
		end
		
		-- access cell you are in
		local x = flr(b.x/8) + 1
		local y = flr(b.y/8) + 1
		if (
				x > 0 and 
				x < 16 and 
				y > 0 and 
				y < 16
		) then
		  if (rows[x][y] and b.tool) then
		    new_cell = rows[x][y]
		    
		    if (
		    		new_cell.ready_count <= 0 
		    		and b.tool.name == "scythe"
		      and new_cell.index != 2
		    ) then
		      new_cell.index = 2
		      cash += 100
		    end
		    
		    if (b.tool.name == "hoe") then
		    		new_cell = do_hoe(rows[x][y])
		    end
		    
		    if (b.tool.name == "seeds") then
		      new_cell = do_seeds(rows[x][y])
		    end  
		    
		    if (b.tool.name == "watering can") then
		      new_cell = do_watering_can(rows[x][y])
		    end
		    
		    rows[x][y] = new_cell
		  end
		end
		
		if (check_ball_collision(next_x, next_y, p.x,p.y,p.w,p.h)) then
		  if (deflect_ball_box(p.x,p.y,p.w,p.h)) then
		    b.dx = -(b.dx + b.dv)
		  else
		  		b.dy = -(b.dy + b.dv)
		  end
		end
		
		for i=1,16 do
				for j=1,16 do
					 local cell = rows[i][j] 
					 local xx = (i-1)*8
					 local yy = (j-1)*8
					 if (cell.water_count > 0) then
					   cell.ready_count -= 1
					 end
					 
					 if (
					 		cell.index >= 33 
					 		and cell.index < 37
					 ) then
					
					 if (check_ball_collision(next_x,next_y,xx,yy,6,6)) then
		  				if (b.tool.name == "pickaxe") then
		  				  cell.index = 1
		  				end
		  				debug = next_x
		  				debug2 = b.x
		  				if (deflect_ball_box(xx,y,8,8)) then
		    				b.dx = -b.dx
		  				else
		  						b.dy = -b.dy
		  				end
		  				
		  				next_x = b.x + b.dx
								next_y = b.y + b.dy
		  				
		  				line(debug,0,debug,128,15)
		  				pset(next_x,next_y,10)
		  				rectfill(xx,yy,xx+7,yy+7)
		  				sfx(0)
		  				--stop()
		  				break
						end
						end
				end
		end
		
		b.x = next_x
		b.y = next_y
end

function check_ball_collision(bx, by, box_x,box_y,box_w,box_h)
		if (by - b.r > box_y + box_h) then
		  --print("a")
		  return false
		end
		
		if (by + b.r < box_y) then
		 -- print("b")
		  return false
		end
		
		if (bx - b.r > box_x + box_w) then
		  --print("c")
		  return false
		end
		
		if (bx + b.r < box_x) then
		  --print("d")
		  return false
		end
	
		return true
end

function deflect_ball_box(x,y,w,h)
  if (b.dx == 0) then
    return false
  elseif (b.dy == 0) then
    return false
  else
    local slp = b.dy / b.dx
    local cx, cy
    
    if (slp > 0 and b.dx > 0) then
      cx = x-b.x
      cy = y-b.y
      
      if (cx <= 0) then
        return false
      elseif (cy/cx < slp) then
        return true
      else
        return false
      end
    elseif (slp < 0 and b.dx > 0) then
      cx = x-b.x
      cy = y+b.y
      
      if (cx <= 0) then
        return false
      elseif (cy/cx < slp) then
        return false
      else
        return true
      end
    elseif (slp > 0 and b.dx < 0) then
      cx = x+w-b.x
      cy = y+h-b.y
      
      if (cx >= 0) then
        return false
      elseif (cy/cx < slp) then
        return false
      else
        return true
      end
    else
      cx = x+w-b.x
      cy = y+h-b.y
      
      if (cx >= 0) then
        return false
      elseif (cy/cx < slp) then
        return false
      else
        return true
      end
    end
  end
  
  return false
end

function do_hoe(cell)
  if (cell.index != 1) then
    return cell
  end
  
  cell.index = 2
  
  return cell
end

function do_seeds(cell)
  if (cell.index != 2) then
    return cell
  end
  
  cell.index = 3
  
  return cell
end

function do_watering_can(cell)
  if (cell.index < 3 or cell.index > 9) then
    return cell
  end
  
  cell.index += 1
  if (cell.ready_count > 0) then
    cell.water_count = 100
  end
  
  return cell
end

function _draw()
		cls()
		
  -- draw all cells
  for i=1,16 do
				for j=1,16 do
					 local cell = rows[i][j] 
					 -- do stuff with the cell
					 spr(cell.index,(i*8)-8,(j*8)-8)
				end
		end
		
		-- draw hud
		circfill(120, 120, 12, 15)
		rectfill(0, 119, 128, 128, 15)
		spr(b.tool.sprite, 112, 110, 2, 2)
		
		if (cash < goal) then
		print(
				"$"..cash.." out of $"..goal,
				4,
				121,
				0
		)
		else
		  print("press — to advance",4,121,0)
		end
		
		-- draw ball and paddle
		circfill(b.x, b.y, b.r, b.tool.c)
		rectfill(p.x, p.y, p.x+p.w, p.y+p.h, p.c)
end
